    <UseWPF>true</UseWPF>
    <UserSecretsId>b863d547-151d-48df-84b8-e1ef733661df</UserSecretsId>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
